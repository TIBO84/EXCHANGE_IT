
<h1>Dashboard <%= "#{@current_user.first_name} #{@current_user.last_name}" %></h1>

<h2>Demandes d'échange:</h2>

<% @shifts.each do |shift| %>

  <div class="shift">
    <%= raw(ap shift) %>
    <p><%= link_to "Proposer un échange", shift_answer_path(shift.id) %></p>
  </div>

<% end %>


<%= link_to "My Shifts", my_shifts_path  %>
<%= link_to "Nouvelle demande", shift_new_path %>
<%= link_to "My Answers", my_answers_path %>
